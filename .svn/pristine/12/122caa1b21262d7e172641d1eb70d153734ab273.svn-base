	<!-- 以下为工具生成SQL，可直接使用，修改需加履历 -->
	<select id="selectByPrimaryKey" parameterType="java.math.BigDecimal"
		resultMap="BaseResultMap">
	SELECT * 
	  FROM
		  (select
		    <include refid="Base_Column_List" />
		     from TBL_COMPANY
		    where ID = #{id,jdbcType=DECIMAL}
		   ) com
      LEFT JOIN 
	      ( SELECT SUM(rate1) AS rate1,SUM(rate2) AS rate2,COUNT(id) AS rateCnt,company_id
	          FROM tbl_comment
	         WHERE state=1
	         GROUP BY company_Id
	        ) coment
	    ON com.id = coment.company_id
	</select>
	<!-- 联合查询 -->
	<select id="selectByMonitorIdquery" parameterType="java.util.HashMap"
		resultMap="BaseResultMap">
	SELECT * 
	  FROM
		  (select
		    <include refid="Base_Column_List" />
		     from TBL_COMPANY
		    where ID in (
		    	select company_id
				from TBL_COMPANY_MONITOR_RELATION
				where monitor_id = #{monitorId,jdbcType=DECIMAL}
		    )
		   ) com
      LEFT JOIN 
	      ( SELECT SUM(rate1) AS rate1,SUM(rate2) AS rate2,COUNT(id) AS rateCnt,company_id
	          FROM tbl_comment
	         WHERE state=1
	         GROUP BY company_Id
	        ) coment
	    ON com.id = coment.company_id
	</select>
	
