/*企业用户导航栏样式*/
img{vertical-align:middle;}
em{font-style:normal;}
a img, :link img, :visited img{border:0;}
a:link, a:visited{text-decoration:none;}
a:hover,a:focus{text-decoration:underline;color:#0573bd;text-decoration:none;}
ul, ul li{list-style-type:none;}

#tops_bg{
border: 1px solid #000099;
background-image: url(../images/lt.png);
background-repeat: repeat-x;
width: 99.9%;
height: 37px;
}


.tops{
	width:Auto;
	height:40px;
	margin:0 auto;
}

.nav_z{
	width:auto!important;
	height:40px;
	float:left;position:relative;
	z-index:999 ;
	}
#navul li{
	float:left;
	width:90px;
	height:37px;
	position:relative;
	text-align:center;
	line-height:37px;
	margin-left: 10px;
	}
#navul li a:link, #navul li a:visited{
	color:#FFF;
	font-weight:500;
	letter-spacing:2px;
	}
/*二级菜单的样式  */
#navul li ul{
	display:none;
	position:absolute;
	z-index:999;
	background:#e6e6e6;
	border:#c6c6c6 1px solid;
	border-bottom:none;
	left:-1px;
	}
#navul li ul li{
	margin:0px;
	display:block;
	width:89px;
	float:none;
	height:35px;
	position:relative;
	line-height:35px;
	border-bottom:#C6C6C6 1px solid;
	font-size:14px;
	overflow:hidden
	}
/*鼠标移动时  */
#navul li.navmoon{
	color:white;
	background:#1C86EE;
	height:38px;
	font-size:14px;
	}
/*菜单被选中时的属性  */
#navul li a.active {
	background:#1C86EE;
	color:white;
	font-weight: bold;
	font-size: 14px;
	height: 37px;
	line-height:37px;
	display: block;
	width:78px;
	}
#navul li.navmoon a{
	color:#404a58;
	}
#navul li.navmoon ul{
	display:block;
	}
#navul li.navmoon ul a{
	display:block;
	width:100px;
	height:35px;
	line-height:35px;
	}
/*二级菜单的hover属性  */
#navul li.navmoon ul a:hover{
	color:#0573bd;
	background:white;
	}

 